<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.project.dao.user.FeederMapper">
	
	<!-- getFeederList -->
	<select id="getFeederList" resultType="feeder" parameterType="feeder">
		select * from feeder 
    </select>
    
	<!-- getFeeder -->
	<select id="getFeeder" resultType="feeder" parameterType="feeder">
		select * from feeder where id = #{id} 
    </select>
	
</mapper> 
